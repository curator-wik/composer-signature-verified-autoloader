{
    "name": "curator-wik/signature-verified-autoloader",
    "description": "Composer plugin that makes the autoloader verify source files as they enter the opcache",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^1.0|^2.0",
        "ext-sodium": "*"
    },
    "autoload": {
        "psr-4": {
            "Curator\\ComposerSAPlugin\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Baynton",
            "email": "mike@mbaynton.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "class": "Curator\\ComposerSAPlugin\\Plugin"
    }
}
